	i2c_mux: i2c-mux {
		compatible = "i2c-gpio";
		sda-gpios = <&gpio4 RK_PA7 (GPIO_ACTIVE_HIGH|GPIO_OPEN_DRAIN)>;
		scl-gpios = <&gpio4 RK_PA6 (GPIO_ACTIVE_HIGH|GPIO_OPEN_DRAIN)>;
		i2c-gpio,delay-us = <8>;
		#address-cells = <1>;
		#size-cells = <0>;
		status = "disabled";
	};
};

