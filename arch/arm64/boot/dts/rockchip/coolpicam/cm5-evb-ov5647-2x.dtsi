 /*mipi csi*/
 
&i2c3 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&i2c3m0_xfer>;
	clock-frequency = <100000>;
	ov5647_2: ov5647-2@36 {
                status = "okay";
                compatible = "ovti,ov5647";
                reg = <0x36>;
				clocks = <&cru CLK_MIPI_CAMARAOUT_M0>;
                clock-names = "xvclk";
				power-domains = <&power RK3588_PD_VI>;
				dovdd-supply = <&vcc_mipidphy0>;
				rockchip,camera-module-index = <1>;
				rockchip,camera-module-facing = "front";
				rockchip,camera-module-name = "THDS11073LarganCAM0";
				rockchip,camera-module-lens-name = "40122a1";

                port {
                        ov5647_out2: endpoint {
                                remote-endpoint = <&mipidphy0_in_ucam1>;
                                data-lanes = <1 2>;
                        };
                };
        };

};

&i2c4 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&i2c4m3_xfer>;
	clock-frequency = <100000>;
	ov5647_1: ov5647-1@36 {
                status = "okay";
                compatible = "ovti,ov5647";
                reg = <0x36>;
				clocks = <&cru CLK_MIPI_CAMARAOUT_M0>;
                clock-names = "xvclk";
				power-domains = <&power RK3588_PD_VI>;
				dovdd-supply = <&vcc_mipidphy0>;
				rockchip,camera-module-index = <1>;
				rockchip,camera-module-facing = "front";
				rockchip,camera-module-name = "THDS11073LarganCAM0";
				rockchip,camera-module-lens-name = "40122a1";

                port {
                        ov5647_out1: endpoint {
                                remote-endpoint = <&mipidphy0_in_ucam0>;
                                data-lanes = <1 2>;
                        };
                };
        };

};

&csi2_dphy0_hw {
	status = "okay";
};

&csi2_dphy1 {
	status = "okay";

	ports {
		#address-cells = <1>;
		#size-cells = <0>;
		port@0 {
			reg = <0>;
			#address-cells = <1>;
			#size-cells = <0>;

			mipidphy0_in_ucam0: endpoint@1 {
				reg = <1>;
				remote-endpoint = <&ov5647_out1>;
				data-lanes = <1 2>;
			};

		};
		port@1 {
			reg = <1>;
			#address-cells = <1>;
			#size-cells = <0>;

			csidphy0_out: endpoint@0 {
				reg = <0>;
				remote-endpoint = <&mipi2_csi2_input>;
			};
		};
	};
};

